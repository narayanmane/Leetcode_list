class Solution {
    public int search(int[] nums, int target) {
      /*  int left=0;
        int right=nums.lenght-1;*/
        int n=nums.length;
        for(int i=0;i<n;i++)
        {
            if(nums[i]==target)
            {
                return i;
            }
            
        }
        
        return -1;
    }
}
