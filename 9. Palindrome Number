class Solution {
    public boolean isPalindrome(int x) {
        int num=0;
        int y=x;
        if(x<0)
            return false;
        while(x>0)
        {
            num=num*10;
            num=num+(x%10);
            x=x/10;
        }
        if(num==y){
            return true;
        }
        else
            return false;
        
    }
}
